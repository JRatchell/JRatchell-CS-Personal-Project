<section>
    <%for (entry of data){%>
        <span>
            <p><%= entry.title%></p>
            <p><%= entry.author%></p>
            <p><%= entry.task%></p>
        </span>
        <span>
            <form action="/tasks/<%=entry._id%>?_method=DELETE" enctype="application/x-www-form-urlencoded" class="inline">
                <input type="submit"/>
            </form>
        </span>
    <%}%>
</section>